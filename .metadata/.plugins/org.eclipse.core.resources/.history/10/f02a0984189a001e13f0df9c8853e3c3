
public class Collections {
	public static void main(String[] args) {
		int number = 10;
		String words = "Hello";
		int wordCount = 0; {
		
		for (int i = 0; i < words.length(); i++) {
			if (Character.isLetter(words.charAt(i))) {
				wordCount++;
		}
	}
		
		if(wordCount > number) {
			System.out.println(true);
		} else {
			System.out.println(false);
		}

}
}
}